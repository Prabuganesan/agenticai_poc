{
  "name": "kodivian",
  "version": "1.0.0",
  "description": "Kodivian Server",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "bin": {
    "kodivian": "./bin/run"
  },
  "files": [
    "bin",
    "marketplaces",
    "dist",
    "npm-shrinkwrap.json",
    "oclif.manifest.json",
    "oauth2.html"
  ],
  "oclif": {
    "bin": "kodivian",
    "commands": "./dist/commands"
  },
  "scripts": {
    "build": "tsc && gulp && (oclif manifest || true)",
    "start": "run-script-os",
    "clean": "rimraf dist",
    "nuke": "rimraf dist node_modules .turbo",
    "start:windows": "cd bin && run start",
    "start:default": "cd bin && ./run start",
    "start-worker:windows": "cd bin && run worker",
    "start-worker:default": "cd bin && ./run worker",
    "start-scheduler:windows": "cd bin && run scheduler",
    "start-scheduler:default": "cd bin && ./run scheduler",
    "user": "run-script-os",
    "user:windows": "cd bin && run user",
    "user:default": "cd bin && ./run user",
    "dev": "nodemon",
    "oclif-dev": "run-script-os",
    "oclif-dev:windows": "cd bin && dev start",
    "oclif-dev:default": "cd bin && ./dev start",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "pnpm build && oclif manifest && oclif readme",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:migration-generate": "pnpm typeorm migration:generate -d ./src/utils/typeormDataSource.ts",
    "typeorm:migration-run": "pnpm typeorm migration:run -d ./src/utils/typeormDataSource.ts",
    "typeorm:migration-revert": "pnpm typeorm migration:revert -d ./src/utils/typeormDataSource.ts",
    "watch": "tsc --watch",
    "version": "oclif readme && git add README.md",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3030 cypress:run",
    "cypress:ci": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test start https-get://localhost:3030 cypress:run",
    "test": "jest --runInBand --detectOpenHandles --forceExit"
  },
  "keywords": [],
  "homepage": "https://kodivian.com",
  "author": {
    "name": "Kodivian",
    "email": "kodivian@kodivian.com"
  },
  "engines": {
    "node": "22.21.1"
  },
  "license": "SEE LICENSE IN LICENSE.md",
